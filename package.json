{
  "name": "babel-plugin-transform-package",
  "version": "1.0.3",
  "description": "Add $packageName and $packageVersion to ES6 classes ",
  "license": "MIT",
  "main": "lib/index.js",
  "repository": "https://github.com/saas-plat/babel-plugin-transform-package",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1"
  },
  "scripts": {
    "compile": "babel src --out-dir lib --source-maps",
    "watch": "babel --watch   src --out-dir lib --source-maps",
    "prepare": "npm run compile",
    "pub": "npm publish --registry http://registry.npmjs.org",
    "build": "babel-plugin build",
    "push": "babel-plugin publish",
    "test": "babel-plugin test"
  },
  "keywords": [
    "babel-plugin"
  ]
}
